{
  "homepage": "http://PumbBank.github.io/design-system-ng",
  "name": "@mill/ng",
  "version": "0.10.64",
  "scripts": {
    "start": "start-storybook -p 6006 -s ./.storybook/favicon && npm run lint",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "build": "npm run build:clear && npm run ng-high-memory && npm run build:package && npm run build:storybook",
    "build:package": "ng build mill-ng && node copy-assets",
    "build:storybook": "build-storybook -s ./.storybook/favicon -c .storybook -o dist",
    "build:clear": "rimraf dist && rimraf dist-package",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/common": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@mill/figma": "1.0.1032021164412",
    "core-js": "3.6.5",
    "ngx-clipboard": "13.0.1",
    "simplebar-angular": "2.3.0",
    "text-mask-core": "5.1.2",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.1",
    "@angular-devkit/build-ng-packagr": "0.1002.1",
    "@angular/cli": "10.2.1",
    "@angular/compiler": "10.2.4",
    "@angular/compiler-cli": "10.2.4",
    "@angular/platform-browser": "10.2.4",
    "@angular/platform-browser-dynamic": "10.2.4",
    "@babel/core": "7.11.1",
    "@jsdevtools/readdir-enhanced": "6.0.4",
    "@storybook/addon-actions": "6.1.17",
    "@storybook/addon-docs": "6.1.17",
    "@storybook/addon-knobs": "6.1.17",
    "@storybook/addon-links": "6.1.17",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-storysource": "6.1.17",
    "@storybook/addons": "6.1.17",
    "@storybook/angular": "6.1.17",
    "@storybook/theming": "6.1.17",
    "@types/node": "14.0.27",
    "babel-loader": "8.1.0",
    "codelyzer": "6.0.0",
    "copyfiles": "^2.4.1",
    "directory-tree": "^2.2.7",
    "eslint-plugin-sonarjs": "0.5.0",
    "fs-extra": "9.0.1",
    "gh-pages": "3.1.0",
    "ng-packagr": "10.0.3",
    "rimraf": "3.0.2",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-scss": "3.18.0",
    "tslint": "5.20.1",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.9.7"
  }
}
